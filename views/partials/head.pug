    -webkit-animation: n2SimpleWhite .6s linear infinite;
    }
    @keyframes n2SimpleWhite {
    to {
    transform: rotate(360deg);
    }
    }
    @-webkit-keyframes n2SimpleWhite {
    to {
    -webkit-transform: rotate(360deg);
    }
    }
