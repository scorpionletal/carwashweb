    .n2-ss-spinner-simple-white:before {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 20px;
    height: 20px;
    margin-top: -11px;
    margin-left: -11px;
    }
    .n2-ss-spinner-simple-white:not(:required):before {
    content: '';
    border-radius: 50%;
